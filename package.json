		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^3.14.1",
		"@remix-run/dev": "^1.6.8",
		"@remix-run/eslint-config": "^1.6.8",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"eslint": "^8.22.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^4.7.4",
